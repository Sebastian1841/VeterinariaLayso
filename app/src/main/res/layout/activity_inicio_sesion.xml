<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/twocack"
    tools:context=".MainActivity">


  <EditText
      android:id="@+id/et_Usuario"
      android:layout_width="316dp"
      android:layout_height="46dp"
      android:ems="10"
      android:hint="@string/et_Usuario"
      android:inputType="textPersonName"
      android:textSize="24sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.505"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.416" />


  <EditText
      android:id="@+id/et_pass"
      android:layout_width="317dp"
      android:layout_height="48dp"
      android:ems="10"
      android:hint="@string/et_pass"
      android:inputType="textPassword"
      android:textSize="24sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.519" />

  <Button
      android:id="@+id/btn_inicio"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:onClick="IniciarSesion"
      android:text="@string/btn_incio"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/et_Usuario"
      app:layout_constraintVertical_bias="0.372" />

  <ProgressBar
      android:id="@+id/pb_inicio"
      style="?android:attr/progressBarStyle"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:visibility="gone"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.498"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/btn_inicio"
      app:layout_constraintVertical_bias="0.404" />

  <ImageView
      android:id="@+id/imageView"
      android:layout_width="251dp"
      android:layout_height="132dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.166"
      app:srcCompat="@mipmap/vat" />

  <ImageView
      android:id="@+id/imageView2"
      android:layout_width="389dp"
      android:layout_height="340dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.253"
      app:srcCompat="@mipmap/login" />

  <ImageView
      android:id="@+id/imageView7"
      android:layout_width="70dp"
      android:layout_height="65dp"
      android:onClick="Home2"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.498"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="1.0"
      app:srcCompat="@mipmap/casa" />
</androidx.constraintlayout.widget.ConstraintLayout>